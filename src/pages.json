  "preloadRule": { "pages/index/index": { "network": "all", "packages": ["pageA"] } },
  "globalStyle": {
    "navigationBarTextStyle": "white",
    "navigationBarTitleText": "demo",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  }
}
