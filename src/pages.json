{
  "pages": [
    { "path": "pages/index/index", "style": { "navigationBarTitleText": "uni-app" } },
    { "path": "pages/vue/vue", "style": { "navigationBarTitleText": "uni-app" } }
  ],
  "subPackages": [
    {
      "root": "pageA",
      "pages": [{ "path": "index/index", "style": { "navigationBarTitleText": "uni-app" } }]
